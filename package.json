    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.4",
    "gulp-usemin": "^0.3.28",
    "gulp-watch": "^5.0.1",
    "gulp-wrap": "^0.14.0",
    "gulp-zip": "^2.0.3",
    "preprocessify": "^1.0.1",
    "vinyl-buffer": "1.0.1",
    "vinyl-source-stream": "2.0.0",
